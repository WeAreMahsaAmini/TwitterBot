// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id    String  @id @map("_id") @db.ObjectId
  username String  @unique
  name  String
  protected Boolean
  verified Boolean

  accessToken String
  refreshToken String
}
